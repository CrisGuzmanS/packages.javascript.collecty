{
  "name": "collecty",
  "version": "2.0.4",
  "description": "allows iterating an array of elements with an easy-to-read syntax and gives functionality to the collection and the elements it iterates.",
  "main": "dist/Collecty.js",
  "types": "dist/Collecty.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrisGuzmanS/packages.javascript.collecty"
  },
  "author": "Cristian Guzm√°n",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "api",
    "array",
    "collection",
    "contains",
    "count",
    "filter",
    "first",
    "firstWhere",
    "fromArray",
    "isEmpty",
    "iterable",
    "json",
    "laravel",
    "list",
    "map",
    "push",
    "concat"
  ]
}